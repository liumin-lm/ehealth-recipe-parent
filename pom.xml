<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>ngari</groupId>
        <artifactId>ehealth-parent</artifactId>
        <version>4.2.2.4.master-SNAPSHOT</version>
    </parent>
    <artifactId>ehealth-recipe-parent</artifactId>
    <version>4.1.1.9.test-SNAPSHOT</version>


    <packaging>pom</packaging>
    <name>root</name>
    <description>处方项目</description>
    <modules>
        <module>ehealth-recipe-entity</module>
        <module>ehealth-recipe-api</module>
        <module>ehealth-recipe-common</module>
        <module>ehealth-recipe-dao</module>
        <module>ehealth-recipe-manager</module>
        <module>ehealth-recipe-client</module>
        <module>ehealth-recipe-atop</module>
        <module>ehealth-recipe-core</module>
    </modules>
    <properties>
        <ngari.voucher.version>4.1.1.9.master-SNAPSHOT</ngari.voucher.version>
        <ngari.es.version>4.1.1.9.master-SNAPSHOT</ngari.es.version>
        <ngari.miscellany.version>4.1.1.9.master-SNAPSHOT</ngari.miscellany.version>
        <ngari.basic.version>4.1.1.9.master-SNAPSHOT</ngari.basic.version>
        <ngari.jgpt.version>4.1.1.9.master-SNAPSHOT</ngari.jgpt.version>
        <ngari.followup.version>4.1.1.9.master-SNAPSHOT</ngari.followup.version>
        <ngari.cdr.version>4.1.1.9.master-SNAPSHOT</ngari.cdr.version>
        <ngari.recipeaudit.version>4.1.1.9.master-SNAPSHOT</ngari.recipeaudit.version>
        <ehealth-pay-api.version>4.1.1.9.master-SNAPSHOT</ehealth-pay-api.version>
        <ngari.consult.version>4.1.1.9.master-SNAPSHOT</ngari.consult.version>
        <ngari.networkclinic.version>4.1.1.9.master-SNAPSHOT</ngari.networkclinic.version>
        <ngari.hisapi.version>4.1.2.6.master-SNAPSHOT</ngari.hisapi.version>
        <ngari.baseapi.version>4.1.1.9.master-SNAPSHOT</ngari.baseapi.version>
        <ngari.opbase.version>4.1.1.9.master-SNAPSHOT</ngari.opbase.version>
        <ngari.utils.version>4.1.1.9.master-SNAPSHOT</ngari.utils.version>
        <ngari.casign.version>4.1.1.9.master-SNAPSHOT</ngari.casign.version>
        <ngari.video.version>4.1.1.9.master-SNAPSHOT</ngari.video.version>
        <ngari.weixin.service.version>4.1.1.9.master-SNAPSHOT</ngari.weixin.service.version>
        <ngari.infra.version>4.1.1.9.master-SNAPSHOT</ngari.infra.version>
        <ngari.message.version>4.1.1.9.master-SNAPSHOT</ngari.message.version>
        <ngari.bill.version>4.1.1.9.master-SNAPSHOT</ngari.bill.version>
        <ngari.account.version>4.1.1.9.master-SNAPSHOT</ngari.account.version>
        <ngari.basicdto.version>4.1.1.9.master-SNAPSHOT</ngari.basicdto.version>
        <ngari.revisit.version>4.1.1.9.master-SNAPSHOT</ngari.revisit.version>
        <ngari.entity.version>4.1.1.9.master-SNAPSHOT</ngari.entity.version>

    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.3</version>
                <configuration>
                    <source>${java.version}</source>
                    <target>${java.version}</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <version>2.4</version>
                <executions>
                    <execution>
                        <id>attach-source</id>
                        <phase>package</phase>
                        <goals>
                            <goal>jar-no-fork</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <version>2.8.2</version>
                <configuration>
                    <skip>${skip_maven_deploy}</skip>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
